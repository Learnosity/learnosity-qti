{
    "name": "learnosity/learnosity-qti",
    "type": "library",
    "description": "Learnosity QTI Conversion Library",
    "keywords": [
        "learnosity",
        "qti",
        "tei"
    ],
    "license" : [
        "GPL-2.0"
    ],
    "autoload": {
        "psr-4": {
            "LearnosityQti\\": "src/",
            "LearnosityQti\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/yaml": "^3.4",
        "twig/twig": "^1.34",
        "twig/extensions": "^1.5",
        "qtism/qtism": "^10.0",
        "learnosity/learnosity-sdk-php": "^0.10"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^5.2",
        "phpunit/phpunit": "^6.0"
    },
    "bin": [
        "bin/mo"
    ]
}
